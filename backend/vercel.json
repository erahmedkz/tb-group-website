{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/uploads/(.*)",
            "dest": "/uploads/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/src/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}